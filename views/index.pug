extends layout.pug
block content
    form#createTokenForm(action='/newToken' method='POST')
        label Token name
            input(name='name')
        label Token symbol
            input(name='symbol')
        button#createToken  create new token


block prepend javaScript
    script.
        window.mainContractAddress = '#{mainContractAddress}';
        window.contactABI = !{contactABI};